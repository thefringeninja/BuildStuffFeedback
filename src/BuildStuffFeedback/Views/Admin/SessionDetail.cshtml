@inherits Nancy.ViewEngines.Razor.NancyRazorViewBase<BuildStuffFeedback.Models.Admin.SessionDetail>
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
</head>
<body>
    <form action="~/admin/session/@Model.SessionId/bulk-feedback" method="POST">
        <div>
            <label>Count</label>
            <input type="text" name="count" />
        </div>
        <div>
            <label>Rating</label>
            <div>
                <label>Green</label>
                <input type="radio" name="rating" value="Green" />
                <label>Yello</label>
                <input type="radio" name="rating" value="Green" />
                <label>Red</label>
                <input type="radio" name="rating" value="Green" />
            </div>
        </div>
        <input type="Submit" value="Submit" />
    </form>
</body>
</html>
